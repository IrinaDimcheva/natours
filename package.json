{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "engines": {
    "node": ">=20.9.0 <21.0.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "set NODE_ENV=production&&nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch --no-hmr ./public/js/index.js --dist-dir ./public/js/bundled",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IrinaDimcheva/natours.git"
  },
  "author": "Irina Dimcheva",
  "bugs": {
    "url": "https://github.com/IrinaDimcheva/natours/issues"
  },
  "homepage": "https://github.com/IrinaDimcheva/natours#readme",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pug": "^3.0.2",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "stripe": "^14.11.0",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "ndb": "^1.1.5",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  }
}
